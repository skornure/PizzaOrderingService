@{
    ViewData["Title"] = "About Us";
}

<h2>About Us</h2>
<div class="row">
    <div id="map"></div>
    <script>
        var map;
        function initMap() {
            map = new google.maps.Map(document.getElementById('map'), {
                center: { lat: 49.986199, lng: 36.233116 },
                zoom: 8
            });
        }
    </script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyA5SIo_FcZbBwzF9eJZK5HIDP0fIwK5BrE&callback=initMap"
            async defer></script>
</div>